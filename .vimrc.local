syntax enable
set background=dark
let g:solarized_termcolors=256
colorscheme solarized
let mapleader=","
map <leader>n :NERDTreeToggle<CR>
:imap jj <Esc>
:imap kj <Esc>
:imap jk <Esc>
:imap kk <Esc>
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
set tabstop=4 shiftwidth=4 expandtab
